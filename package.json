{
  "name": "student-company-matching-platform",
  "version": "1.0.0",
  "description": "Enterprise-grade student-company matching platform with AI-powered scoring",
  "main": "dist/server.js",
  "scripts": {
    "dev": "npm run dev:backend",
    "dev:backend": "nodemon --watch src --ext ts --exec \"ts-node -r dotenv/config src/server.ts\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:phase1": "nodemon --exec ts-node src/server.ts --env-file .env.phase1",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc && npm run prisma:generate",
    "build:frontend": "cd frontend && npm run build",
    "start": "node dist/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:phase1": "jest --testPathPattern=phase1 --coverage",
    "test:phase2": "jest --testPathPattern=phase2 --coverage",
    "test:phase3": "jest --testPathPattern=phase3 --coverage",
    "test:integration": "jest --testPathPattern=integration --coverage",
    "test:all-phases": "npm run test:phase1 && npm run test:phase2 && npm run test:phase3 && npm run test:integration",
    "test:e2e": "cd frontend && npm run test:e2e",
    "test:setup": "npm run test:db:setup && npm run test:seed",
    "test:db:setup": "DATABASE_URL=$DATABASE_TEST_URL npx prisma migrate reset --force --skip-seed",
    "test:seed": "ts-node tests/seed/test-data.ts",
    "test:cleanup": "ts-node scripts/test-cleanup.ts",
    "test:comprehensive": "ts-node scripts/run-comprehensive-tests.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:phase1": "prisma migrate dev --schema prisma/schema.prisma",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "docker:build": "docker build -t student-matching-platform .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "phase1:setup": "npm run prisma:migrate:phase1 && npm run prisma:generate",
    "phase1:dev": "npm run phase1:setup && npm run dev:phase1",
    "phase1:test": "npm run test:phase1",
    "phase2:setup": "npm run prisma:migrate && npm run prisma:generate && ts-node scripts/seed-phase2.ts",
    "phase2:dev": "npm run phase2:setup && npm run dev",
    "phase2:seed": "ts-node scripts/seed-phase2.ts",
    "phase3:setup": "npm run prisma:migrate && npm run prisma:generate && npm run seed:reference && ts-node scripts/seed-phase2.ts && ts-node scripts/seed-phase3.ts",
    "phase3:dev": "npm run phase3:setup && npm run dev",
    "phase3:seed": "ts-node scripts/seed-phase3.ts",
    "seed:reference": "ts-node scripts/seed-reference-data.ts",
    "models:generate": "npm run prisma:generate"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/axios": "^0.9.36",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.3.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "redis": "^4.6.12",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/pdf-parse": "^1.1.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "student-matching",
    "recruitment",
    "enterprise",
    "typescript",
    "react",
    "postgresql"
  ],
  "author": "Enterprise Development Team",
  "license": "MIT"
}
